{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "cypress/globals": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:cypress/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["cypress"],
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "off"
  }
}
